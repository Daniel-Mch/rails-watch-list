<div class="container">

<h1>Create new bookmark</h1>

<!-- http://localhost:3000/lists/1/bookmarks/new -->

<%= simple_form_for [@list, @bookmark]  do |f| %>
  <%= f.association :movie %>
  <%= f.input :comment %>
  <div class="btn btn-primary">
  <%= f.button :submit %>
  </div>
<% end %>

<%= link_to "Back to list", root_path(@list)  %>

</div>
